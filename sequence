class comp_decomp_sequence extends uvm_sequence #(transaction);

  `uvm_object_utils(comp_decomp_sequence)

  function new(string name = "comp_decomp_sequence");
    super.new(name);
  endfunction

  virtual task body();
    `uvm_info("SEQUENCE", "Starting comp_decomp_sequence", UVM_MEDIUM)
    
    for (int i = 0; i < 10; i++) begin
      transaction trans = transaction::type_id::create("trans");
      trans.randomize();
      start_item(trans);
      finish_item(trans);
    end
    
    `uvm_info("SEQUENCE", "Completed comp_decomp_sequence", UVM_MEDIUM)
  endtask
endclass
